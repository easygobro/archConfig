[module/ewmh]
type = internal/xworkspaces

; Only show workspaces defined on the same output as the bar
;
; Useful if you want to show monitor specific workspaces
; on different bars
;
; Default: false
pin-workspaces = false

; Create click handler used to focus desktop
; Default: true
enable-click = true

; Create scroll handlers used to cycle desktops
; Default: true
enable-scroll = true

;       v     

icon-1 = 1;
icon-2 = 2;
icon-3 = 3;
icon-4 = 4;
icon-5 = 5;
icon-6 = 6;
icon-7 = 7;
icon-8 = 8;
icon-9 = 9;
icon-0 = 0;
icon-10 = scratchpad;
icon-default = " "


format = <label-state> 
label-monitor = %icon% 


label-active =  %icon%
label-active-foreground = #FF0000
label-active-background = #000
label-active-padding = 2

;label-occupied = %icon%
label-occupied-foreground = #FF0000
label-occupied-background = #000
label-occupied-padding = 2

label-urgent = %icon%
label-urgent-foreground = #FF0000
label-urgent-background = #000
;label-urgent-underline = ${colors.alert}
label-urgent-padding = 2


label-empty = %icon%
label-empty-foreground = #008000
label-enpty-background = #00000000
label-empty-padding = 2